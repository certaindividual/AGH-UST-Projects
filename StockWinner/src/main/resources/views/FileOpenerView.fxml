<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="401.0" prefWidth="666.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.FileOpenerController">
    <HBox layoutX="15.0" layoutY="27.0" prefHeight="94.0" prefWidth="638.0" AnchorPane.leftAnchor="15.0">
        <children>
            <Button fx:id="chooseFileButton" mnemonicParsing="false" onAction="#handleChoosingAction" text="Choose File">
            <HBox.margin>
               <Insets left="10.0" top="10.0" />
            </HBox.margin></Button>
         <TextField fx:id="fileLocationField" disable="true" maxWidth="1.7976931348623157E308" prefHeight="30.0" prefWidth="256.0" promptText="File location">
            <HBox.margin>
               <Insets left="5.0" top="10.0" />
            </HBox.margin>
         </TextField>
         <ComboBox fx:id="websitesComboBox" prefWidth="150.0" promptText="Source Website">
            <HBox.margin>
               <Insets left="5.0" top="10.0" />
            </HBox.margin>
         </ComboBox>
         <Button fx:id="openFileButton" mnemonicParsing="false" onAction="#handleOpenAction" text="Open">
            <HBox.margin>
               <Insets left="5.0" top="10.0" />
            </HBox.margin></Button>
        </children>
    </HBox>


</AnchorPane>
